    <div class="container">
      <div class="header">
        <img src="images/banner.jpg">

        <div class="title">
          <p>ADMIN</p>
        </div>
      </div>

      <div class="content">
        <div class="first-content-box">
          <a href="/admin#add-admin"><input id="submit" type="submit" value="Add new admin"></a>
          <a href="/admin#add-manager"><input id="submit" type="submit" value="Add new manager"></a>
          <a href="/admin#add-referee"><input id="submit" type="submit" value="Add new referee"></a>
        </div>

        <div class="logout-box">
          <a href="/logout"><input id="submit" type="submit" value="Logout"></a>
        </div>
      </div>

    </div>
	</div>
</div>

<!-- #add admin -->
<div data-role="page" id="add-admin" data-theme="a" onload="window.location.href = 'https://google.com'">
