<!-- #add manager -->
<div data-role="page" id="add-manager" data-theme="a" onload="window.location.href = 'https://google.com'">
	<div role="main" class="ui-content">
    <div class="container">
      <div class="header">
        <img src="images/banner.jpg">

        <div class="title">
          <p>ADD MANAGER</p>
        </div>
      </div>

      <div class="content">
        <div class="first-content-box">
          <form action="/createManager" data-ajax="false" method="post">

            <p>Username</p>
            <input type="text" name="username" id="username" class="middle no-focus-outline" required></input>
            <p>Password</p>
            <input type="password" name="password" id="password" class="middle" required></input>
            <p>Team Name</p>
            <input type="text" name="teamname" id="teamname" class="middle no-focus-outline" required></input>

            <input id="submit" type="submit" value="Create">

          </form>
        </div>
      </div>
    </div>
	</div>
</div>

